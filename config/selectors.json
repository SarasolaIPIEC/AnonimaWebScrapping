    "zip_input": [
      {"css": "#nro_codigo_postal"},
      {"placeholder": "(?i)(c[oó]digo postal|cp|localidad)"},
      {"css": "input[placeholder*=postal i], input[placeholder*=CP i], input[type=search]"}
    ],
    "result_item": [
      {"css": "#sel_sucursal label:has-text('Ushuaia')"},
      {"css": "label[for^=sucursal_]:has-text('Ushuaia')"},
