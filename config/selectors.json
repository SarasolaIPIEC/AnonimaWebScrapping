    {"css": "header button, header [role=button]"},
    {"text": "(?i)Ushuaia"}
  ],
  "postal_input": [
    {"placeholder": "(?i)(c[oó]digo postal|cp)"},
    {"css": "input[placeholder*=postal i], input[placeholder*=CP i]"}
  ],
  "branch_option": [
    {"text": "(?i)ushuaia\\s*5"}
  ],
  "confirm_button": [
    {"role": "button", "name": "(?i)(confirmar|aplicar|aceptar|guardar)"}
  ],
  "change_location_link": [
    {"css": ".callout_sucursal_in a.btn.btn10"},
    {"text": "(?i)cambiar"}
